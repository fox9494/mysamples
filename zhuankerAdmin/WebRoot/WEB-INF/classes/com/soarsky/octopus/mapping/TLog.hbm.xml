<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.soarsky.octopus.mapping.TLog" table="T_LOG" schema="ZHUANKER">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="sequence">
                <param name="sequence">SEQ_PRI_LOG</param>
            </generator>
        </id>
        <property name="operator" type="java.lang.String">
            <column name="OPERATOR"  />
        </property>
        <property name="content" type="java.lang.String">
            <column name="CONTENT"  />
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS"  />
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="CREATEDATE"  />
        </property>
        
        <property name="exceContent" type="java.lang.String">
            <column name="EXCECONTENT"  />
        </property>
    </class>
</hibernate-mapping>
